{
  "name": "owning-a-home",
  "version": "0.0.1",
  "dependencies": {
    "font-awesome": "~3.2.1",
    "html5shiv": "latest",
    "jquery": "~1.11.0",
    "normalize-css": "~3.0.1",
    "cf-buttons": "git://github.com/cfpb/cf-buttons.git",
    "cf-colors": "git://github.com/cfpb/cf-colors.git",
    "cf-expandables": "git://github.com/cfpb/cf-expandables.git",
    "cf-forms": "git://github.com/cfpb/cf-forms.git",
    "cf-grid": "git://github.com/cfpb/cf-grid.git#0.8.0",
    "cf-pagination": "git://github.com/cfpb/cf-pagination.git",
    "cf-typography": "git://github.com/cfpb/cf-typography.git",
    "cf-icons": "git://github.com/cfpb/cf-icons.git",
    "highcharts-release": "~3.0.10"
  },
  "exportsOverride": {
    "cf-*": {
      "css": "src/**/*.css",
      "less": "src/**/*.less",
      "fonts": "src/fonts/*.*",
      "js": "src/**/*.js"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
}
